<Window x:Class="_1712928_MiniProject_MultimediaPlayer.SavePlaylist"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_1712928_MiniProject_MultimediaPlayer"
        mc:Ignorable="d"
        Title="SavePlaylist" Height="150" Width="300" ResizeMode="NoResize">
    <Canvas Background="#FFADCBF0">
        <Label Content="Save playing playlist with name...." Height="33" Canvas.Left="10" Canvas.Top="10" Width="245" FontStyle="Italic" FontWeight="Bold" FontSize="14" Foreground="#FF466D7C"/>
        <TextBox Name="namePlaylistToSave" Width="191" Canvas.Left="16" Canvas.Top="48" Height="30"></TextBox>
        <Button Name="SavePlaylistButton" Click="SavePlaylistButton_Click" Content="OK" Height="30" Canvas.Left="232" Canvas.Top="48" Width="37"/>
    </Canvas>
</Window>
